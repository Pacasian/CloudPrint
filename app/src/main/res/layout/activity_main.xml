        android:layout_width="216dp"
        android:layout_height="216dp"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/abc"
        android:id="@+id/splashscreenimg"
        />
    </LinearLayout>

</LinearLayout>
